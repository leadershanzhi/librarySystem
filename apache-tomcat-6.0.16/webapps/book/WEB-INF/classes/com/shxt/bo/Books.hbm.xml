<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.shxt.bo" >
   <class name="Books" table="B_Books">
     <id name="bid" column="B_BID" type="integer">
       <generator class="sequence">
          <param name="sequence">seq_bid</param>
       </generator>
     </id>
     <property name="isbn" column="B_ISBN" type="string"/>
     <property name="name" column="B_NAME" type="string"/>
     <property name="ncount" column="B_NCOUNT" type="integer"/>
     <property name="lcount" column="B_LCOUNT" type="integer"/>
     <property name="type" column="B_TYPE" type="string"/>
     <property name="pub" column="B_PUB" type="string"/>
     <property name="author" column="B_AUTHOR" type="string"/>
     <property name="btime" column="B_BTIME" type="string"/>
     <property name="bpic" column="B_BPIC" type="string"/>
   
   </class>
</hibernate-mapping>
