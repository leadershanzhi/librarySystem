<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.shxt.bo" >
   <class name="Messages" table="B_Messages">
     <id name="mid" column="M_ID" type="integer">
       <generator class="sequence">
          <param name="sequence">seq_mid</param>
       </generator>
     </id>
     <property name="topic" column="M_TOPIC" type="string"/>
     <property name="time" column="M_TIME" type="string"/>
     <property name="type" column="M_TYPE" type="string"/>
     <property name="content" column="M_CONTENT" type="string"/>
     <property name="point" column="M_POINT" type="integer"/>
     <property name="people" column="M_PEOPLE" type="string"/>
     
   
   </class>
</hibernate-mapping>
